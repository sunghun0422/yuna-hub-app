{
  "version": 2,
  "builds": [{ "src": "api/**/*.js", "use": "@vercel/node" }],
  "functions": { "api/**/*.js": { "runtime": "nodejs18.x" } },
  "routes": [
    { "src": "^/api/github-read$", "dest": "/api/github-read.js" },
    { "src": "^/api/github-sync$", "dest": "/api/github-sync.js" },
    { "src": "^/api/post_summarize_url$", "dest": "/api/post_summarize_url.js" },
    { "src": "^/api(/)?$", "dest": "/api/index.js" }
  ]
}
