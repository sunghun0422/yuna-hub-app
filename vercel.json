{
  "version": 2,
  "builds": [
    { "src": "server/index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "^/\\.well-known/ai-plugin\\.json$", "dest": "public/ai-plugin.json" },
    { "src": "^/openapi\\.yaml$", "dest": "public/openapi.yaml" },
    { "src": "^/healthz$", "dest": "server/index.js" },
    { "src": "^/api/(.*)$", "dest": "server/index.js" },
    { "src": "^(.*)$", "dest": "public$1" }
  ]
}
