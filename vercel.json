{
  "version": 2,
  "builds": [
    { "src": "server/index.js", "use": "@vercel/node" },
    { "src": "public/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/healthz", "dest": "/server/index.js" },
    { "src": "/api/(.*)", "dest": "/server/index.js" },
    { "src": "/openapi.yaml", "dest": "/server/index.js" },
    { "src": "/", "dest": "/server/index.js" }
  ]
}
